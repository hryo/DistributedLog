{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "System.Net": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Secondaries": {
    "HealthChecksIntervalSeconds": "2",
    "Urls": [
      "http://distributedlog.replica1",
      "http://distributedlog.replica2"
    ]
  }
}
